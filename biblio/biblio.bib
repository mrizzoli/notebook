@book{ladner_practical_2014,
  address = {Walnut Creek, CA},
  title = {Practical Ethnography: A Guide to Doing Ethnography in the Private Sector},
  isbn = {978-1-61132-389-4 978-1-61132-390-0},
  lccn = {GN450.8 .L33 2014},
